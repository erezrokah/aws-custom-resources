{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["ApiToken", "Owner", "Repo"],
  "properties": {
    "ApiToken": {
      "type": "string"
    },
    "Owner": {
      "type": "string"
    },
    "Repo": {
      "type": "string"
    },
    "PolicyArn": {
      "type": "string"
    },
    "EnvironmentVariables": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "value"],
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    }
  }
}
